{
  "version": 2,
  "builds": [
    {
      "src": "server-oauth.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server-oauth.cjs"
    }
  ],
  "env": {
    "AUTH0_DOMAIN": "@auth0_domain",
    "AUTH0_CLIENT_ID": "@auth0_client_id",
    "AUTH0_CLIENT_SECRET": "@auth0_client_secret",
    "REDIRECT_URI": "@redirect_uri",
    "NGROK_URL": "@ngrok_url"
  }
}
